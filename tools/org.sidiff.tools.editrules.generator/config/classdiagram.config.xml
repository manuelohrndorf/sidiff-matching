<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "http://pi.informatik.uni-siegen.de/SiDiff/Editrulesgeneratorconfig.dtd" >
<Config>
	<GeneralSettings>
		<preventInconsistency value="true" />
		<initialChecks allow="true" />
		<reduceToSuperType SET_UNSET="false" ADD_REMOVE="false" CHANGE="false" />
		<disableVariants value="false"/>
   		<modelUsesProfileMechanism value="false"/>
	</GeneralSettings>
	
	<Transformations>
		<Creates allow="true" />
		<Deletes allow="true" />
		<Moves allow="true" />
		<Sets allow="true" />
		<Unsets allow="true" />
		<Adds allow="true" />
		<Removes allow="true" />
		<Changes allow="true" />
	</Transformations>

	<!-- nested=true: means the root element can also appear
	somewhere in its lower containments (eg. (Sub-)StateMachine)-->
	<Root name="Model" nested="false"/>
	
	<WhiteList>	
		<EClass name="Model" />
		<EClass name="Class" />
		<EClass name="Interface" />
		<EClass name="Package" />
		<EClass name="Operation" />
		<EClass name="Property" />
		<EClass name="Parameter" />
		<EClass name="ParameterSet" />
		<EClass name="PrimitiveType" />
		<EClass name="DataType" />
		<EClass name="Enumeration" />
		<EClass name="EnumerationLiteral" />
		<EClass name="Generalization" />
		<EClass name="Association" />
		<EClass name="Dependency" />
		<EClass name="Type" />
	</WhiteList>
	
	<BlackList>
	</BlackList>
	
</Config>